    <rules>
      <rule name="Icon Rule" prop_id="image" out_exp="false">
        <exp bool_exp="pv0==0&amp;&amp;pv1==0">
          <value>off.png</value>
        </exp>
        <exp bool_exp="pv0==0&amp;&amp;pv1==1">
          <value>down.png</value>
        </exp>
        <exp bool_exp="pv0==1&amp;&amp;pv1==0">
          <value>up.png</value>
        </exp>
        <exp bool_exp="pv0==1&amp;&amp;pv1==1">
          <value>on.png</value>
        </exp>
        <pv trig="true">loc://$(DID)_set(0)</pv>
        <pv trig="true">loc://$(DID)_readback(0)</pv>
      </rule>
      <rule name="Label Rule" prop_id="text" out_exp="false">
        <exp bool_exp="pv0==0&amp;&amp;pv1==0">
          <value>Power is Off</value>
        </exp>
        <exp bool_exp="pv0==0&amp;&amp;pv1==1">
          <value>Ramping Down ...</value>
        </exp>
        <exp bool_exp="pv0==1&amp;&amp;pv1==0">
          <value>Ramping up ...</value>
        </exp>
        <exp bool_exp="pv0==1&amp;&amp;pv1==1">
          <value>Power is On</value>
        </exp>
        <pv trig="true">loc://$(DID)_set(0)</pv>
        <pv trig="true">loc://$(DID)_readback(0)</pv>
      </rule>
    </rules>
